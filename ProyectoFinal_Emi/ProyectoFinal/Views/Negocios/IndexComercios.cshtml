<body>
    <div class="row">
        <div class="col-md-12">
            <div class="panel panel-white" style="width:1190px;">
                <div class="panel-heading">
                    <h2 class="panel-title"><span class="text-bold">Comercios</span></h2>
                </div>
                <div class="panel-body">
                   
                    <br />
                    <br />
                    <ul class="pagination pagination-blue margin-bottom-10">


                        @if (Model != null)
                        {
                            foreach (var item in Model)
                            {
                                int idPrincipal = 0;
                                foreach (var img in item.FotosNegocio)
                                {
                                    if (img.esPrincipal == true)
                                    {
                                        idPrincipal = img.idFoto;
                                    }
                                }



                                int foto = 0;
                                <li class="col-lg-12 panel panel-white border-light">
                            
                                    <div class="col-lg-6">
                                        <img src="/Negocios/ObtenerImagen/@idPrincipal" alt="Alternate Text" style="width: 400px; height: 200px;" />
                                        <br />
                                        <br />
                                        @{
                                foreach (var image in item.FotosNegocio.Take(5))
                                {
                                    if (image.esPrincipal != true)
                                    {
                                        foto = image.idFoto;
                                        @*<img src="@Url.Action("ObtenerImagen","Negocios",new { id = item.idFoto })" alt="Alternate Text" style="width: 75px; height: 75px;" />*@

                                            <img src="/Negocios/ObtenerImagen/@foto" alt="Alternate Text" style="width: 77px; height: 75px;" />



                                    }
                                }
                                        }

                                    </div>


                                    <div class="col-lg-3">
                                        <a style="font-size: 20px;" onclick="window.location='@Url.Action("VerComercio", new { id=item.idNegocio })';"> @item.nombre</a>

                                    </div>

                                    <div class="col-lg-3">
                                        @Html.DisplayFor(modelItem => item.Comercio.FirstOrDefault().Rubro.nombreRubro)
                                        <br />
                                        <br />
                                        <br />
                                        @{

                                int rubro = item.Comercio.FirstOrDefault().Rubro.idRubro;
                                switch (rubro)
                                {
                                    case 1: <i class="fa fa-car fa-5x fa-border"></i>
                                        break;

                                    case 2: <i class="fa fa-female fa-5x fa-border"></i>
                                        break;

                                    case 3: <i class="fa fa-wrench fa-5x fa-border"></i>
                                        break;

                                    case 4: <i class="fa fa-ticket fa-5x fa-border"></i>
                                        break;

                                    case 5: <i class="fa fa-truck fa-5x fa-border"></i>
                                        break;

                                    case 6: <i class="fa fa-cutlery fa-5x fa-border"></i>
                                        break;

                                    case 7: <i class="fa fa-trophy fa-5x fa-border"></i>
                                        break;
                                    case 8: <i class="fa fa-cutlery fa-5x fa-border"></i>
                                        break;
                                    case 9:    <i class="fa fa-male fa-5x fa-border"></i>
                                        break;
                                    case 1002:    <i class="fa fa-ambulance fa-5x fa-border"></i>
                                        break;
                                    case 1003:    <i class="fa fa-building fa-5x fa-border"></i>
                                        break;
                                    case 1004:    <i class="fa fa-shopping-cart fa-5x fa-border"></i>
                                        break;
                                    case 1005:    <i class="fa fa-laptop fa-5x fa-border"></i>
                                        break;
                                    case 1006:    <i class="fa fa-taxi fa-5x fa-border"></i>
                                        break;
                                    case 1007:    <i class="fa fa-mortar-board fa-5x fa-border"></i>
                                        break;
                                    case 1008:    <i class="fa fa-book fa-5x fa-border"></i>
                                        break;
                                    case 1009:    <i class="fa fa-home fa-5x fa-border"></i>
                                        break;
                                    case 1010:    <i class="fa fa-phone fa-5x fa-border"></i>
                                        break;
                                    case 1011:    <i class="fa fa-eye fa-5x fa-border"></i>
                                        break;
                                    case 1012:    <i class="fa fa-glass fa-5x fa-border"></i>
                                        break;
                                    case 1013:    <i class="fa fa-paw fa-5x fa-border"></i>
                                        break;
                                    default: // Codigo para resultado distinto de 1, 2 y 3
                                        break;
                                }
                                        }
                                    </div>




                                </li>

                            }
                        }
                        else
                        {
                            @Html.Label("NO HAY COMERCIOS PUBLICADOS.")
                        }

                     

                    </ul>


                </div>
            </div>
        </div>
    </div>
                   


                   




