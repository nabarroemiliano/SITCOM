<body>
    <div class="row">
        <div class="col-sm-12">
            <div class="panel panel-white">
                <div class="panel-heading">
                    <h4 class="panel-title">Detalle del Usuario: <span class="text-bold">@Model.nombreUsuario</span></h4>
                </div>
                <div class="panel-body">
                    <div>
                        <dl class="dl-horizontal">
                            <dt>
                                Usuario:
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.nombreUsuario)
                            </dd>

                            <dt>
                                Password:
                            </dt>

                            <dd>
                                ********
                            </dd>

                            <dt>
                                Perfil:
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.Perfiles.nombre)
                            </dd>
                        </dl>
                    </div>
                </div>
                <div class="panel-footer">
                    <p>
                        @Html.ActionLink("Editar", "Edit", new { id = Model.idUsuario }) |
                        @Html.ActionLink("Volver", "Index")
                    </p>
                </div>
            </div>
        </div>
    </div>  
